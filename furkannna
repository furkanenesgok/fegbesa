#include <iostream>

int main() {

    // Kitabin toplam sayfa sayisi
    int totalPageCount;

    // Kitabin Turk Lirasi cinsinden fiyati (Orn: 89.99)
    double bookPrice;

    // 10 uzerinden okuyucu puani (Orn: 8.5)
    float readerRating;

    // Kitabin kutuphanede olup olmadigi (var icin true, yok icin false)
    bool isAvailableInLibrary;

    // Kitabin bulundugu rafin harfi (Orn: 'A', 'B', 'C')
    char shelfLetter;

    return 0;
}
